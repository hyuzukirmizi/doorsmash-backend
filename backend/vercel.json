{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "*.py": {
      "runtime": "python3.12",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/api/chat", "destination": "/chatbot_api.py" },
    { "source": "/api/:match*", "destination": "/main.py" }
  ]
}
